{
  "name": "flippy-panda",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:os": "npm run build:osx ; npm run build:linux",
    "build:osx": "npx nativefier -n flippy-panda -p osx -i src/favicon.ico --app-version 0.1.0 -f https://tgotwig.github.io/flippy-panda/ bin",
    "build:linux": "npx nativefier -n flippy-panda -p linux -i src/favicon.ico --app-version 0.1.0 https://tgotwig.github.io/flippy-panda/ bin",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4000",
    "deploy": "ng build --prod && npx firebase-tools deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "~10.2.2",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~10.1.3",
    "@angular/material": "^10.2.2",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "firebase": "^7.21.0",
    "firebaseui": "^4.6.1",
    "firebaseui-angular": "^4.1.0",
    "gsap": "^3.5.1",
    "guid-typescript": "^1.0.9",
    "ngx-gravatar": "^8.1.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.62",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}

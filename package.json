{
  "name": "flippy-panda",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:os": "npm run build:osx ; npm run build:linux",
    "build:osx": "npx nativefier -n flippy-panda -p osx -i src/favicon.ico --app-version 0.1.0 -f https://tgotwig.github.io/flippy-panda/ bin",
    "build:linux": "npx nativefier -n flippy-panda -p linux -i src/favicon.ico --app-version 0.1.0 https://tgotwig.github.io/flippy-panda/ bin",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng build --prod --base-href https://tgotwig.github.io/flippy-panda/ && npx angular-cli-ghpages"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "firebase": "^7.8.1",
    "firebaseui": "^4.4.0",
    "firebaseui-angular": "^4.0.1",
    "font-awesome": "^4.7.0",
    "gsap": "^3.1.1",
    "hammerjs": "^2.0.8",
    "human-readable-ids": "^1.0.4",
    "ngx-gravatar": "^8.1.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.7.5"
  }
}
